:Approved_Name_ICNP rdf:type owl:Class ;
                    
                    rdfs:subClassOf :Acceptable_Under_Governing_Code ;
                    
                    <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Autonym_Avoidance

:Autonym_Avoidance rdf:type owl:Class ;
                   
                   rdfs:subClassOf :Replacement_of_a_name ;
                   
                   <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Available_ICZN

:Available_ICZN rdf:type owl:Class ;
                
                rdfs:subClassOf :Acceptable_Under_Governing_Code ;
                
                <http://purl.org/dc/elements/1.1/type> "Term" ;
                
                <http://purl.org/dc/elements/1.1/description> "Nomenclatural act statuses that are available names under the International Code of Zoological Nomenclature." .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Change_of_Type

:Change_of_Type rdf:type owl:Class ;
                
                rdfs:subClassOf :NomenclaturalEventType ;
                
                <http://purl.org/dc/elements/1.1/type> "Category" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Change_of_combination

:Change_of_combination rdf:type owl:Class ;
                       
                       rdfs:subClassOf :NomenclaturalEventType ;
                       
                       <http://purl.org/dc/elements/1.1/type> "Category" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Changed_Combination_ICZN

:Changed_Combination_ICZN rdf:type owl:Class ;
                          
                          rdfs:subClassOf :Change_of_combination ,
                                          :TaxonomicEventType ;
                          
                          <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Conservation

:Conservation rdf:type owl:Class ;
              
              rdfs:subClassOf :NomenclaturalEventType ;
              
              <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Conserved_Name

:Conserved_Name rdf:type owl:Class ;
                
                rdfs:subClassOf :Acceptable_Under_Governing_Code ;
                
                rdfs:comment "A name that should not be acceptable under the governing code, but which has been conserved by an action of the relevant code governed body."^^xsd:string ;
                
                :example "Hemerobius elegans Guérin-Méneville, 1844"^^xsd:string ;
                
                <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Corrected_Name

:Corrected_Name rdf:type owl:Class ;
                
                rdfs:subClassOf :Acceptable_Under_Governing_Code ;
                
                <http://purl.org/dc/elements/1.1/title> "nomen correctum"^^xsd:string ;
                
                <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Creation_of_Justifed_Emendation

:Creation_of_Justifed_Emendation rdf:type owl:Class ;
                                 
                                 rdfs:subClassOf :Replacement_of_a_name ;
                                 
                                 <http://purl.org/dc/elements/1.1/title> "ICZN"^^xsd:string ;
                                 
                                 <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Creation_of_Unjustified_Emendation

:Creation_of_Unjustified_Emendation rdf:type owl:Class ;
                                    
                                    rdfs:subClassOf :Replacement_of_a_name ;
                                    
                                    rdfs:comment "ICZN:  Any change in spelling other than a Justified Emendation"^^xsd:string ;
                                    
                                    <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Doubtfull_Name

:Doubtfull_Name rdf:type owl:Class ;
                
                rdfs:subClassOf :Dubious_Name ;
                
                rdfs:comment "ICNP.   A name of doubtfull or uncertain application."^^xsd:string ;
                
                <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Dubious_Name

:Dubious_Name rdf:type owl:Class ;
              
              rdfs:subClassOf :NomenclaturalActStatus ;
              
              rdfs:comment """Hawksworth:  \"A name of unknown or doubtfull application.\"  

 Names for which the available circumscription information is too limited to allow the name to be related to a taxonomic concept."""^^xsd:string ;
              
              <http://purl.org/dc/elements/1.1/title> "nomen dubium"^^xsd:string ;
              
              <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Earlier_Homonym

:Earlier_Homonym rdf:type owl:Class ;
                 
                 rdfs:subClassOf :Valid_ICBN ;
                 
                 rdfs:comment "ICBN"^^xsd:string ;
                 
                 <http://purl.org/dc/elements/1.1/type> "Term" ;
                 
                 rdfs:seeAlso :Later_Homonym .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Earlier_Nomenclatural_Synonym

:Earlier_Nomenclatural_Synonym rdf:type owl:Class ;
                               
                               rdfs:subClassOf :Valid_ICBN ;
                               
                               <http://purl.org/dc/elements/1.1/type> "Term" ;
                               
                               rdfs:seeAlso :Later_Nomenclatural_Synonym .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Earlier_Taxonomic_Synonym

:Earlier_Taxonomic_Synonym rdf:type owl:Class ;
                           
                           rdfs:subClassOf :Accepted_Name ;
                           
                           rdfs:comment """\"a name based on a different type from that of the accepted name of taxon; a subjective synonym in zool. and prok.; see also taxonomic synonym; syntaxonomic synonym.\"
Hawksworth, Terms Used in Bionomenclature."""^^xsd:string ;
                           
                           <http://purl.org/dc/elements/1.1/type> "Term" ;
                           
                           rdfs:seeAlso :Later_Taxonomic_Synonym .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Effectively_Published

:Effectively_Published rdf:type owl:Class ;
                       
                       rdfs:subClassOf :Published ;
                       
                       :example "A work has met the criteria of the ICBN or ICNP to have been published."^^xsd:string ;
                       
                       <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#First_Revisor_Choosing_Spelling

:First_Revisor_Choosing_Spelling rdf:type owl:Class ;
                                 
                                 rdfs:subClassOf :Replacement_of_a_name ;
                                 
                                 <http://purl.org/dc/elements/1.1/type> "Term" .



