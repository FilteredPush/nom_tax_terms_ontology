                              owl:hasValue :Available_ICZN
                            ] ;
                
                skos:broader :Available_ICZN ;

                skos:scopeNote "ICZN"@en ,
                               "ICNP"@en ;

                skos:related :Earlier_Homonym ;
                
                :appliesInGoverningCode :ICNP ,
