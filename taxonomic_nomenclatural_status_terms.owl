                                          skos:Concept ;

                                 rdfs:domain [ rdf:type owl:Restriction ;
                                               owl:onProperty :hasNomenclaturalStatus ;
                                               owl:hasValue :Replacement_of_a_name 
                                 ] ;

                                 skos:prefLabel "First Revisor, Choosing Spelling"@en ; 

                                 skos:definition "The first revisor of a zoological name which is spelled in more than one way in the original work selecting the correct spelling for the name, under ICZN Article 24.2.3. 'Selection of correct original spellings. If a name is spelled in more than one way in the original work, the first author to have cited them together and to have selected one spelling as correct is the First Reviser'." @en ;                                  

                                 skos:scopeNote "ICZN"@en ;
                                 
                                 dc:type "Term" ;
                                 
                                 :appliesInGoverningCode :ICZN ;
                                 
                                 skos:broader :Replacement_of_a_name .



