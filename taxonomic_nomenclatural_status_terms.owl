###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Absolute_Synonym

:Absolute_Synonym rdf:type owl:Class ;
                  
                  rdfs:subClassOf :Not_Acceptable_Under_Governing_Code ;
                  
                  <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Acceptable_Under_Governing_Code

:Acceptable_Under_Governing_Code rdf:type owl:Class ;
                                 
                                 rdfs:subClassOf :NomenclaturalActStatus ;
                                 
                                 owl:disjointWith :Not_Acceptable_Under_Governing_Code ;
                                 
                                 rdfs:comment "Statuses of nomenclatural acts that are acceptable statuses under the governing code."^^xsd:string ;
                                 
                                 <http://purl.org/dc/elements/1.1/type> "Category" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Accepted_Name

:Accepted_Name rdf:type owl:Class ;
               
               rdfs:subClassOf :TaxonomicEventStatus ;
               
               <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Alternative_name

:Alternative_name rdf:type owl:Class ;
                  
                  rdfs:subClassOf :NomenclaturalActStatus ;
                  
                  rdfs:comment "ICBN"^^xsd:string ;
                  
                  <http://purl.org/dc/elements/1.1/description> "One of a set of two or more different names proposed at the same time by the same author for the same taxon."^^xsd:string ;
                  
                  <http://purl.org/dc/elements/1.1/title> "nom. alt."^^xsd:string ;
                  
                  <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Ambiguuous_Name

:Ambiguuous_Name rdf:type owl:Class ;
                 
                 rdfs:subClassOf :Not_Acceptable_Under_Governing_Code ;
                 
                 <http://purl.org/dc/elements/1.1/type> "Term" .



