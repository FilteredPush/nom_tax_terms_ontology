###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#appliesInGoverningCode

:appliesInGoverningCode rdf:type owl:ObjectProperty ;
                        
                        rdfs:range :NomenclaturalCode ;
                        
                        rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#conservedInFavorOf

:conservedInFavorOf rdf:type owl:ObjectProperty ;
                    
                    rdfs:subPropertyOf :hasPriority ;
                    
                    owl:inverseOf :rejectedInFavorOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#hasPriority

:hasPriority rdf:type owl:ObjectProperty ;
             
             rdfs:comment "One name of the pair should be used because it has nomenclatural priority over the other."^^xsd:string ;
             
             rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isEarlierHomonymOf

:isEarlierHomonymOf rdf:type owl:ObjectProperty ;
                    
                    rdfs:domain :Earlier_Homonym ;
                    
                    rdfs:range :Later_Homonym ;
                    
                    rdfs:subPropertyOf :hasPriority .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isEarlierNomenclaturalSynonymOf

:isEarlierNomenclaturalSynonymOf rdf:type owl:ObjectProperty ;
                                 
                                 rdfs:subPropertyOf :hasPriority ;
                                 
                                 owl:inverseOf :isLaterNomenclaturalSynonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isEarlierTaxonomicSynonymOf

:isEarlierTaxonomicSynonymOf rdf:type owl:ObjectProperty ;
                             
                             owl:inverseOf :isLaterTaxonomicSynonymOf ;
                             
                             rdfs:subPropertyOf :isSubjectivelySeniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorHomonymOf

:isJuniorHomonymOf rdf:type owl:ObjectProperty ;
                   
                   rdfs:domain :Junior_Homonym ;
                   
                   rdfs:range :Senior_Homonym ;
                   
                   rdfs:subPropertyOf :lacksPriority .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorObjectiveSynonymOf

:isJuniorObjectiveSynonymOf rdf:type owl:ObjectProperty ;
                            
                            rdfs:domain :Junior_Objective_Synonym ;
                            
                            rdfs:range :Senior_Objective_Synonym ;
                            
                            rdfs:subPropertyOf :lacksPriority .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorPrimaryHomonymOf

:isJuniorPrimaryHomonymOf rdf:type owl:ObjectProperty ;
                          
                          rdfs:domain :Junior_Primary_Homonym ;
                          
                          rdfs:range :Senior_Primary_Homonym ;
                          
                          rdfs:subPropertyOf :isJuniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorSecondaryHomonymOf

:isJuniorSecondaryHomonymOf rdf:type owl:ObjectProperty ;
                            
                            rdfs:domain :Junior_Secondary_Homonym ;
                            
                            rdfs:range :Senior_Secondary_Homonym ;
                            
                            rdfs:subPropertyOf :isJuniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorSubjectiveSynonymOf

:isJuniorSubjectiveSynonymOf rdf:type owl:ObjectProperty ;
                             
                             rdfs:domain :Junior_Subjective_Synonym ;
                             
                             rdfs:range :Senior_Subjective_Synonym ;
                             
                             rdfs:subPropertyOf :isSubjectivelyJuniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isLaterHomonymOf

:isLaterHomonymOf rdf:type owl:ObjectProperty ;
                  
                  rdfs:range :Earlier_Homonym ;
                  
                  rdfs:domain :Later_Homonym ;
                  
                  owl:inverseOf :isEarlierHomonymOf ;
                  
                  rdfs:subPropertyOf :lacksPriority .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isLaterNomenclaturalSynonymOf

:isLaterNomenclaturalSynonymOf rdf:type owl:ObjectProperty ;
                               
                               rdfs:domain :Earlier_Nomenclatural_Synonym ;
                               
                               rdfs:range :Later_Nomenclatural_Synonym ;
                               
                               rdfs:subPropertyOf :lacksPriority .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isLaterTaxonomicSynonymOf

:isLaterTaxonomicSynonymOf rdf:type owl:ObjectProperty ;
                           
                           rdfs:domain :Earlier_Taxonomic_Synonym ;
                           
                           rdfs:range :Later_Taxonomic_Synonym ;
                           
                           rdfs:subPropertyOf :isSubjectivelyJuniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorHomonymOf

:isSeniorHomonymOf rdf:type owl:ObjectProperty ;
                   
                   rdfs:subPropertyOf :hasPriority ;
                   
                   owl:inverseOf :isJuniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorObjectiveSynonymOf

:isSeniorObjectiveSynonymOf rdf:type owl:ObjectProperty ;
                            
                            rdfs:subPropertyOf :hasPriority ;
                            
                            owl:inverseOf :isJuniorObjectiveSynonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorPrimaryHomonym

:isSeniorPrimaryHomonym rdf:type owl:ObjectProperty ;
                        
                        owl:inverseOf :isJuniorPrimaryHomonymOf ;
                        
                        rdfs:subPropertyOf :isSeniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorSecondaryHomonym

:isSeniorSecondaryHomonym rdf:type owl:ObjectProperty ;
                          
                          owl:inverseOf :isJuniorSecondaryHomonymOf ;
                          
                          rdfs:subPropertyOf :isSeniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorSubjectiveSynonymOf

:isSeniorSubjectiveSynonymOf rdf:type owl:ObjectProperty ;
                             
                             owl:inverseOf :isJuniorSubjectiveSynonymOf ;
                             
                             rdfs:subPropertyOf :isSubjectivelySeniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSubjectivelyJuniorTo

:isSubjectivelyJuniorTo rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment "An assertion that one name of a pair should not be used for subjective/taxonomic reasons."^^xsd:string ;
                        
                        rdfs:subPropertyOf :lacksPriority .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSubjectivelySeniorTo

:isSubjectivelySeniorTo rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment "An assertion that one name of a pair should  be used in favor of the other for subjective/taxonomic reasons."^^xsd:string ;
                        
                        rdfs:subPropertyOf :hasPriority ;
                        
                        owl:inverseOf :isSubjectivelyJuniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#lacksPriority

:lacksPriority rdf:type owl:ObjectProperty ;
               
               rdfs:comment "One name of the pair should not be used because it lacks priority (and is not used for nomenclatural reasons)."^^xsd:string ;
               
               owl:inverseOf :hasPriority ;
               
               rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#rejectedInFavorOf

:rejectedInFavorOf rdf:type owl:ObjectProperty ;
                   
                   rdfs:range :Conserved_Name ;
                   
                   rdfs:domain :Rejected_Name ;
                   
                   rdfs:subPropertyOf :lacksPriority .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#resultsInNomenclaturalStatus

:resultsInNomenclaturalStatus rdf:type owl:ObjectProperty ;
                              
                              rdfs:range :NomenclaturalActStatus ;
                              
                              rdfs:domain :NomenclaturalEventType ;
                              
                              rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.w3.org/2002/07/owl#topObjectProperty

owl:topObjectProperty rdf:type owl:ObjectProperty .



###  http://www.w3.org/2004/02/skos/core#related

skos:related rdf:type owl:ObjectProperty .





