                                rdfs:subClassOf :NomenclaturalEventType ;
                                
                                <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Forgotten_Name

:Forgotten_Name rdf:type owl:Class ;
                
                rdfs:subClassOf :Nomen_Oblitum ;
                
                rdfs:comment "ICZN Glossary: nomen oblitum: applied after 1 January 2000 to a name, unused since 1899, which as a result of an action taken under Article 23.9.2 does not take precedence over a younger synonym or homonym in prevailing usage; the younger name which takes precedence over the nomen oblitum may be called a nomen protectum (q.v.). "^^xsd:string ;
                
                <http://purl.org/dc/elements/1.1/title> "nomen oblitum"^^xsd:string ;
                
                <http://purl.org/dc/elements/1.1/type> "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Historical_Synonym

:Historical_Synonym rdf:type owl:Class ;
                    
                    rdfs:subClassOf :Not_Acceptable_Under_Governing_Code ;
                    
                    rdfs:comment "ICNP.  Synonyms not included in the Approved List.  Generally placed in single quotes when included in a synonymy.  "^^xsd:string ;
                    
                    <http://purl.org/dc/elements/1.1/type> "Term" .



