      rdfs:subClassOf :NomenclaturalCode ;
      
      <http://purl.org/dc/elements/1.1/title> "International Code of Botanical Nomenclature"^^xsd:string ;
      
      <http://purl.org/dc/elements/1.1/type> "Term" .



