#################################################################
#
#    Object Properties
#
#################################################################


###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#action

:action rdf:type owl:ObjectProperty .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#appliesInGoverningCode

:appliesInGoverningCode rdf:type owl:ObjectProperty ;
                        
                        rdfs:range :NomenclaturalCode ;
                        
                        rdfs:subPropertyOf owl:topObjectProperty .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#approvedListIncludes

:approvedListIncludes rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "ICNP Approved List Includes"@en ;
                      
                      rdfs:subPropertyOf :action ;
                      
                      rdfs:range [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasGoverningCode ;
                                   owl:hasValue :ICNP
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasNomenclaturalStatus ;
                                   owl:hasValue :Approved_Name_ICNP
                                 ] .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#conservedOver

:conservedOver rdf:type owl:IrreflexiveProperty ,
                        owl:ObjectProperty ;
               
               rdfs:subPropertyOf :hasPriorityOver .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#creationOfNomenAbortivum

:creationOfNomenAbortivum rdf:type owl:ObjectProperty ;
                          
                          rdfs:subPropertyOf :action .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#effectiveNomenclaturalAct

:effectiveNomenclaturalAct rdf:type owl:ObjectProperty ;
                           
                           rdfs:subPropertyOf :action .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#effectivePublicationOf

:effectivePublicationOf rdf:type owl:ObjectProperty ;
                        
                        rdfs:subPropertyOf :action .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#hasGoverningCode

:hasGoverningCode rdf:type owl:ObjectProperty .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#hasNomenclaturalStatus

:hasNomenclaturalStatus rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "Has nomenclatural status"@en ;
                        
                        rdfs:range :NomenclaturalActStatus .

###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isNomenclaturalEventType

:isNomenclaturalEventType rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "isNomenclaturalEventType"@en ;
                        
                        rdfs:range :NomenclaturalEventType .

###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isTaxonomicEventType

:isTaxonomicEventType rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "isTaxonomicEventType"@en ;
                        
                        rdfs:range :TaxonomicEventType .

###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#hasTaxonomicStatus

:hasTaxonomicStatus rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "Has taxonomic status"@en ;

                        rdfs:range :NomenclaturalActStatus .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#hasPriorityOver

:hasPriorityOver rdf:type owl:ObjectProperty ;
                 
                 rdfs:comment "One name of the pair should be used because it has nomenclatural priority over the other."^^xsd:string ;
                 
                 rdfs:subPropertyOf :relatedToName .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isEarlierHomonymOf

:isEarlierHomonymOf rdf:type owl:IrreflexiveProperty ,
                             owl:ObjectProperty ;
                    
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Earlier_Homonym
                                ] ;
                    
                    rdfs:range  [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Later_Homonym
                                ] ;
                    
                    rdfs:subPropertyOf :hasPriorityOver ;
                    
                    owl:inverseOf :isLaterHomonymOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isEarlierNomenclaturalSynonymOf

:isEarlierNomenclaturalSynonymOf rdf:type owl:IrreflexiveProperty ,
                                          owl:ObjectProperty ;
                                 
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Earlier_Nomenclatural_Synonym
                                ] ;
                    
                    rdfs:range  [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Later_Nomenclatural_Synonym
                                ] ;

                                 rdfs:subPropertyOf :hasPriorityOver ;
                                 
                                 owl:inverseOf :isLaterNomenclaturalSynonymOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isEarlierTaxonomicSynonymOf

:isEarlierTaxonomicSynonymOf rdf:type owl:IrreflexiveProperty ,
                                      owl:ObjectProperty ;
                             
                             owl:inverseOf :isLaterTaxonomicSynonymOf ;
                             
                             rdfs:subPropertyOf :isSubjectivelySeniorTo .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorHomonymOf

:isJuniorHomonymOf rdf:type owl:ObjectProperty ;
                   
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Junior_Homonym
                                ] ;
                    
                    rdfs:range  [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Senior_Homonym
                                ] ;
                   
                   rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorObjectiveSynonymOf

:isJuniorObjectiveSynonymOf rdf:type owl:IrreflexiveProperty ,
                                     owl:ObjectProperty ;
                            
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Junior_Objective_Synonym
                                ] ;
                    
                    rdfs:range  [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Senior_Objective_Synonym
                                ] ;
                            
                            owl:inverseOf :isSeniorObjectiveSynonymOf ;
                            
                            rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorPrimaryHomonymOf

:isJuniorPrimaryHomonymOf rdf:type owl:IrreflexiveProperty ,
                                   owl:ObjectProperty ;
                          
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Junior_Primary_Homonym
                                ] ;
                    
                    rdfs:range  [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Senior_Primary_Homonym
                                ] ;
                          
                          rdfs:subPropertyOf :isJuniorHomonymOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorSecondaryHomonymOf

:isJuniorSecondaryHomonymOf rdf:type owl:IrreflexiveProperty ,
                                     owl:ObjectProperty ;
                            
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Junior_Secondary_Homonym
                                ] ;
                    
                    rdfs:range  [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Senior_Secondary_Homonym
                                ] ;
                            
                            rdfs:subPropertyOf :isJuniorHomonymOf ;
                            
                            owl:inverseOf :isSeniorSecondaryHomonym .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorSubjectiveSynonymOf

:isJuniorSubjectiveSynonymOf rdf:type owl:IrreflexiveProperty ,
                                      owl:ObjectProperty ;
                             
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasTaxonomicStatus ;
                                  owl:hasValue :Junior_Subjective_Synonym
                                ] ;
                    
                    rdfs:range  [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasTaxonomicStatus ;
                                  owl:hasValue :Senior_Subjective_Synonym
                                ] ;
                             
                             owl:inverseOf :isSeniorSubjectiveSynonymOf ;
                             
                             rdfs:subPropertyOf :isSubjectivelyJuniorTo .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isLaterHomonymOf

:isLaterHomonymOf rdf:type owl:IrreflexiveProperty ,
                           owl:ObjectProperty ;
                  
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Later_Homonym
                                ] ;

                    rdfs:range [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Earlier_Homonym
                                ] ;
                  
                  rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isLaterNomenclaturalSynonymOf

:isLaterNomenclaturalSynonymOf rdf:type owl:IrreflexiveProperty ,
                                        owl:ObjectProperty ;
                               
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Earlier_Nomenclatural_Synonym 
                                ] ;

                    rdfs:range [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Later_Nomenclatural_Synonym 
                                ] ;
                               
                               rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isLaterTaxonomicSynonymOf

:isLaterTaxonomicSynonymOf rdf:type owl:IrreflexiveProperty ,
                                    owl:ObjectProperty ;
                           
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasTaxonomicStatus ;
                                  owl:hasValue :Earlier_Taxonomic_Synonym 
                                ] ;

                    rdfs:range [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasTaxonomicStatus ;
                                  owl:hasValue :Later_Taxonomic_Synonym 
                                ] ;
                           
                           rdfs:subPropertyOf :isSubjectivelyJuniorTo .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorHomonymOf

:isSeniorHomonymOf rdf:type owl:ObjectProperty ;
                   
                   rdfs:subPropertyOf :hasPriorityOver ;
                   
                   owl:inverseOf :isJuniorHomonymOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorObjectiveSynonymOf

:isSeniorObjectiveSynonymOf rdf:type owl:IrreflexiveProperty ,
                                     owl:ObjectProperty ;
                            
                            rdfs:subPropertyOf :hasPriorityOver .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorPrimaryHomonym

:isSeniorPrimaryHomonym rdf:type owl:IrreflexiveProperty ,
                                 owl:ObjectProperty ;
                        
                        owl:inverseOf :isJuniorPrimaryHomonymOf ;
                        
                        rdfs:subPropertyOf :isSeniorHomonymOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorSecondaryHomonym

:isSeniorSecondaryHomonym rdf:type owl:IrreflexiveProperty ,
                                   owl:ObjectProperty ;
                          
                          rdfs:subPropertyOf :isSeniorHomonymOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorSubjectiveSynonymOf

:isSeniorSubjectiveSynonymOf rdf:type owl:IrreflexiveProperty ,
                                      owl:ObjectProperty ;
                             
                             rdfs:subPropertyOf :isSubjectivelySeniorTo .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSubjectivelyJuniorTo

:isSubjectivelyJuniorTo rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment "An assertion that one name of a pair should not be used for subjective/taxonomic reasons."^^xsd:string ;
                        
                        owl:inverseOf :isSubjectivelySeniorTo ;
                        
                        rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSubjectivelySeniorTo

:isSubjectivelySeniorTo rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment "An assertion that one name of a pair should  be used in favor of the other for subjective/taxonomic reasons."^^xsd:string ;
                        
                        rdfs:subPropertyOf :hasPriorityOver .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#lacksPriorityInFavorOf

:lacksPriorityInFavorOf rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment "One name of the pair should not be used because it lacks priority (and is not used for nomenclatural reasons)."^^xsd:string ;
                        
                        owl:inverseOf :hasPriorityOver ;
                        
                        rdfs:subPropertyOf :relatedToName .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#rejectedInFavorOf

:rejectedInFavorOf rdf:type owl:IrreflexiveProperty ,
                            owl:ObjectProperty ;
                   
                    rdfs:domain [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Rejected_Name 
                                ] ;

                    rdfs:range [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNomenclaturalStatus ;
                                  owl:hasValue :Conserved_Name
                                ] ;
                   
                   owl:inverseOf :conservedOver ;
                   
                   rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#relatedToName

:relatedToName rdf:type owl:ObjectProperty .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#resultsInNomenclaturalStatus

:resultsInNomenclaturalStatus rdf:type owl:ObjectProperty ;
                              
                              rdfs:range :NomenclaturalActStatus ;
                              
                              rdfs:domain :NomenclaturalEventType ;
                              
                              rdfs:subPropertyOf :action .



###  http://www.w3.org/2002/07/owl#topObjectProperty

owl:topObjectProperty rdf:type owl:ObjectProperty .



###  http://www.w3.org/2004/02/skos/core#broader

skos:broader rdf:type owl:ObjectProperty .



###  http://www.w3.org/2004/02/skos/core#related

skos:related rdf:type owl:ObjectProperty .





#################################################################
#
#    Classes
#
#################################################################



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Acceptable_Under_Governing_Code

:Acceptable_Under_Governing_Code rdf:type owl:Class ;
                                 
                                 rdfs:subClassOf :NomenclaturalActStatus ;
                                 
                                 owl:disjointWith :Not_Acceptable_Under_Governing_Code ;
                                 
                                 rdfs:comment "Statuses of nomenclatural acts that are acceptable statuses under the governing code."^^xsd:string ;
                                 
                                 dc:type "Category" .


###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Change_of_Type

:Change_of_Type rdf:type owl:Class ;
                
                rdfs:subClassOf :NomenclaturalEventType ;

                rdfs:comment "Category of nomenclatural events in which the type for a name is changed"@en ; 

                rdfs:comment "May not affect the name."@en ;
                
                dc:type "Category" .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Nomen_Imaginatum

:Nomen_Imaginatum rdf:type owl:Class ;
                  
                  skos:prefLabel "nomen imaginatum"@la ;

                  skos:altLabel "imaginary name"@en ;
                  
                  rdfs:comment "Imagniary name used to illustrate a nomenclatural point."^^xsd:string ;
                  
                  :example "Aus bus Jones"^^xsd:string ;

                  :example "Peziza araneosa Apocr."^^xsd:string ;
                  
                  dc:title "nomen imagniatum"@la .




###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#NomenclaturalActStatus

:NomenclaturalActStatus rdf:type owl:Class ;
                        
                        rdfs:comment "ICZN Glossary: Nomenclatural Status: \"Of a name, nomenclatural act or work: its standing in nomenclature (i.e. its availability or otherwise, and in the case of a name its spelling, the typification of the nominal taxon it denotes, and its precedence relative to other names).\""^^xsd:string ;
                        
                        dc:description "Terms describing the status of nomenclatural acts/events (loosely, terms describing the nomenclatural, but not taxonomic, status of names in biological nomenclature)."^^xsd:string ;
                        
                        dc:type "Category" .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#NomenclaturalCode

:NomenclaturalCode rdf:type owl:Class ;
                   
                   dc:description "Codes that govern biological nomenclature."^^xsd:string ;
                   
                   dc:type "Category" .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#NomenclaturalEventType

:NomenclaturalEventType rdf:type owl:Class ;
                        
                        rdfs:comment "ICZN Glossary:  \"A published act which affects the nomenclatural status (q.v.) of a scientific name or the typification of a nominal taxon.\""^^xsd:string ;
                        
                        dc:description "Terms for types of events in biological nomenclature."^^xsd:string ;
                        
                        dc:type "Category" .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Not_Acceptable_Under_Governing_Code

:Not_Acceptable_Under_Governing_Code rdf:type owl:Class ;
                                     
                                     rdfs:subClassOf :NomenclaturalActStatus ;
                                     
                                     dc:type "Category" .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Replacement_of_a_name

:Replacement_of_a_name rdf:type owl:Class ;
                       
                       rdfs:subClassOf :NomenclaturalEventType ;
                       
                       dc:type "Category" .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#TaxonomicEventStatus

:TaxonomicEventStatus rdf:type owl:Class ;
                      
                      rdfs:comment "Terms for the taxonomic status of biological names."^^xsd:string ;
                      
                      dc:type "Category" .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#TaxonomicEventType

:TaxonomicEventType rdf:type owl:Class ;
                    
                    rdfs:comment "Terms describing types of taxonomic events."^^xsd:string ;
                    
                    dc:type "Category" .



###  http://www.w3.org/2002/07/owl#Thing

owl:Thing rdf:type owl:Class .



###  http://www.w3.org/2004/02/skos/core#Concept

skos:Concept rdf:type owl:Class .



#################################################################
#
#    Individuals
#
#################################################################


###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Absolute_Synonym

:Absolute_Synonym rdf:type owl:NamedIndividual ,
                           :Not_Acceptable_Under_Governing_Code ,
                           skos:Concept ;
                  
                  dc:description "A general term for synonyms based upon the same type."^^xsd:string ,
                                                                "Hawksworth: \"A homotypic synonym, a nomenclatural synonym (botany), an objective synonym (zoology). \""^^xsd:string ;
                  
                  rdfs:comment "Implied in calling a name an absolute synonym is that it is a Junior/Later Absolute Synonym"^^xsd:string ;
                  
                  dc:type "Term" ;

                  skos:definition "A general term for synonyms based upon the same type."^^xsd:string ;
                  
                  rdfs:domain [ rdf:type owl:Restriction ;
                                owl:onProperty :hasNomenclaturalStatus ;
                                owl:hasValue :Nomen_Abortivum
                              ] ;
                        
                  skos:scopeNote "ICBN"@en ,
                                 "ICNB"@en ,
                                 "ICZN"@en ;
                  
                  :appliesInGoverningCode :ICBN ,
                                          :ICNP ,
                                          :ICZN ;
                  
                  skos:broader :Nomen_Abortivum .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Acceptable_Under_Governing_Code

:Acceptable_Under_Governing_Code rdf:type owl:NamedIndividual ,
                                          skos:Concept ;
                                 
                                 rdfs:comment "Statuses of nomenclatural acts that are acceptable statuses under the governing code."^^xsd:string ;
                                 
                                 dc:type "Category" .


