@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix : <http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@base <http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl> .

<http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl> rdf:type owl:Ontology ;
                                                                                 
                                                                                 owl:versionInfo 0.5 ;
                                                                                 
                                                                                 dc:description "An ontological treatment of terms for Nomenclatural Acts/Events, Nomenclatural Events, and terms for the Nomenclatural Status and Taxonomic Status that can result from such events.  "^^xsd:string ;
                                                                                 
                                                                                 dc:subject "Biological Nomenclature"^^xsd:string ;
                                                                                 
                                                                                 owl:versionInfo "Draft."^^xsd:string ;
                                                                                 
                                                                                 rdfs:comment """Informed by the following sources:

http://software.speciesfile.org/Design/TaxaTables.aspx
http://clade.ansp.org/obis/mollusc_keywords.php
http://bionomenclature-glossary.gbif.org/

The Codes:
http://www.nhm.ac.uk/hosted-sites/iczn/code/
http://ibot.sav.sk/icbn/main.htm
http://www.ncbi.nlm.nih.gov/books/NBK8817/"""^^xsd:string ;
                                                                                 
                                                                                 dc:contributor "Paul J. Morris"^^xsd:string ;

                                                                                 dc:contributor "Robert A. Morris"^^xsd:string ;

                                                                                 dc:creator "Paul J. Morris"^^xsd:string .


#################################################################
#
#    Annotation properties
#
#################################################################


dc:creator rdf:type owl:AnnotationProperty .



owl:versionInfo rdf:type owl:AnnotationProperty .



rdfs:seeAlso rdf:type owl:AnnotationProperty .



dc:type rdf:type owl:AnnotationProperty .



dc:subject rdf:type owl:AnnotationProperty .



:example rdf:type owl:AnnotationProperty .



rdfs:label rdf:type owl:AnnotationProperty .



dc:description rdf:type owl:AnnotationProperty .



rdfs:comment rdf:type owl:AnnotationProperty .



dc:title rdf:type owl:AnnotationProperty .


skos:scopeNote rdf:type owl:AnnotationProperty .


skos:definition rdf:type owl:AnnotationProperty .

skos:prefLabel rdf:type owl:AnnotationProperty .

skos:altLabel rdf:type owl:AnnotationProperty .

#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#action

:action rdf:type owl:ObjectProperty .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#appliesInGoverningCode

:appliesInGoverningCode rdf:type owl:ObjectProperty ;
                        
                        rdfs:range :NomenclaturalCode ;
                        
                        rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#conservedOver

:conservedOver rdf:type owl:IrreflexiveProperty ,
                        owl:ObjectProperty ;
               
               rdfs:subPropertyOf :hasPriorityOver ;
               
               owl:inverseOf :rejectedInFavorOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#creationOfNomenAbortivum

:creationOfNomenAbortivum rdf:type owl:ObjectProperty ;
                          
                          rdfs:subPropertyOf :action .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#effectiveNomenclaturalAct

:effectiveNomenclaturalAct rdf:type owl:ObjectProperty ;
                           
                           rdfs:subPropertyOf :action .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#effectivePublicationOf

:effectivePublicationOf rdf:type owl:ObjectProperty ;
                        
                        rdfs:subPropertyOf :action .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#hasPriorityOver

:hasPriorityOver rdf:type owl:ObjectProperty ;
                 
                 rdfs:comment "One name of the pair should be used because it has nomenclatural priority over the other."^^xsd:string ;
                 
                 rdfs:subPropertyOf :relatedPairsOfNames .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isEarlierHomonymOf

:isEarlierHomonymOf rdf:type owl:IrreflexiveProperty ,
                             owl:ObjectProperty ;
                    
                    rdfs:domain :Earlier_Homonym ;
                    
                    rdfs:range :Later_Homonym ;
                    
                    rdfs:subPropertyOf :hasPriorityOver .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isEarlierNomenclaturalSynonymOf

:isEarlierNomenclaturalSynonymOf rdf:type owl:IrreflexiveProperty ,
                                          owl:ObjectProperty ;
                                 
                                 rdfs:subPropertyOf :hasPriorityOver ;
                                 
                                 owl:inverseOf :isLaterNomenclaturalSynonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isEarlierTaxonomicSynonymOf

:isEarlierTaxonomicSynonymOf rdf:type owl:IrreflexiveProperty ,
                                      owl:ObjectProperty ;
                             
                             owl:inverseOf :isLaterTaxonomicSynonymOf ;
                             
                             rdfs:subPropertyOf :isSubjectivelySeniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorHomonymOf

:isJuniorHomonymOf rdf:type owl:ObjectProperty ;
                   
                   rdfs:domain :Junior_Homonym ;
                   
                   rdfs:range :Senior_Homonym ;
                   
                   rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorObjectiveSynonymOf

:isJuniorObjectiveSynonymOf rdf:type owl:IrreflexiveProperty ,
                                     owl:ObjectProperty ;
                            
                            rdfs:domain :Junior_Objective_Synonym ;
                            
                            rdfs:range :Senior_Objective_Synonym ;
                            
                            rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorPrimaryHomonymOf

:isJuniorPrimaryHomonymOf rdf:type owl:IrreflexiveProperty ,
                                   owl:ObjectProperty ;
                          
                          rdfs:domain :Junior_Primary_Homonym ;
                          
                          rdfs:range :Senior_Primary_Homonym ;
                          
                          rdfs:subPropertyOf :isJuniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorSecondaryHomonymOf

:isJuniorSecondaryHomonymOf rdf:type owl:IrreflexiveProperty ,
                                     owl:ObjectProperty ;
                            
                            rdfs:domain :Junior_Secondary_Homonym ;
                            
                            rdfs:range :Senior_Secondary_Homonym ;
                            
                            rdfs:subPropertyOf :isJuniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isJuniorSubjectiveSynonymOf

:isJuniorSubjectiveSynonymOf rdf:type owl:IrreflexiveProperty ,
                                      owl:ObjectProperty ;
                             
                             rdfs:domain :Junior_Subjective_Synonym ;
                             
                             rdfs:range :Senior_Subjective_Synonym ;
                             
                             rdfs:subPropertyOf :isSubjectivelyJuniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isLaterHomonymOf

:isLaterHomonymOf rdf:type owl:IrreflexiveProperty ,
                           owl:ObjectProperty ;
                  
                  rdfs:range :Earlier_Homonym ;
                  
                  rdfs:domain :Later_Homonym ;
                  
                  owl:inverseOf :isEarlierHomonymOf ;
                  
                  rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isLaterNomenclaturalSynonymOf

:isLaterNomenclaturalSynonymOf rdf:type owl:IrreflexiveProperty ,
                                        owl:ObjectProperty ;
                               
                               rdfs:domain :Earlier_Nomenclatural_Synonym ;
                               
                               rdfs:range :Later_Nomenclatural_Synonym ;
                               
                               rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isLaterTaxonomicSynonymOf

:isLaterTaxonomicSynonymOf rdf:type owl:IrreflexiveProperty ,
                                    owl:ObjectProperty ;
                           
                           rdfs:domain :Earlier_Taxonomic_Synonym ;
                           
                           rdfs:range :Later_Taxonomic_Synonym ;
                           
                           rdfs:subPropertyOf :isSubjectivelyJuniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorHomonymOf

:isSeniorHomonymOf rdf:type owl:ObjectProperty ;
                   
                   rdfs:subPropertyOf :hasPriorityOver ;
                   
                   owl:inverseOf :isJuniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorObjectiveSynonymOf

:isSeniorObjectiveSynonymOf rdf:type owl:IrreflexiveProperty ,
                                     owl:ObjectProperty ;
                            
                            rdfs:subPropertyOf :hasPriorityOver ;
                            
                            owl:inverseOf :isJuniorObjectiveSynonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorPrimaryHomonym

:isSeniorPrimaryHomonym rdf:type owl:IrreflexiveProperty ,
                                 owl:ObjectProperty ;
                        
                        owl:inverseOf :isJuniorPrimaryHomonymOf ;
                        
                        rdfs:subPropertyOf :isSeniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorSecondaryHomonym

:isSeniorSecondaryHomonym rdf:type owl:IrreflexiveProperty ,
                                   owl:ObjectProperty ;
                          
                          owl:inverseOf :isJuniorSecondaryHomonymOf ;
                          
                          rdfs:subPropertyOf :isSeniorHomonymOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSeniorSubjectiveSynonymOf

:isSeniorSubjectiveSynonymOf rdf:type owl:IrreflexiveProperty ,
                                      owl:ObjectProperty ;
                             
                             owl:inverseOf :isJuniorSubjectiveSynonymOf ;
                             
                             rdfs:subPropertyOf :isSubjectivelySeniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSubjectivelyJuniorTo

:isSubjectivelyJuniorTo rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment "An assertion that one name of a pair should not be used for subjective/taxonomic reasons."^^xsd:string ;
                        
                        rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#isSubjectivelySeniorTo

:isSubjectivelySeniorTo rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment "An assertion that one name of a pair should  be used in favor of the other for subjective/taxonomic reasons."^^xsd:string ;
                        
                        rdfs:subPropertyOf :hasPriorityOver ;
                        
                        owl:inverseOf :isSubjectivelyJuniorTo .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#lacksPriorityInFavorOf

:lacksPriorityInFavorOf rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment "One name of the pair should not be used because it lacks priority (and is not used for nomenclatural reasons)."^^xsd:string ;
                        
                        owl:inverseOf :hasPriorityOver ;
                        
                        rdfs:subPropertyOf :relatedPairsOfNames .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#rejectedInFavorOf

:rejectedInFavorOf rdf:type owl:IrreflexiveProperty ,
                            owl:ObjectProperty ;
                   
                   rdfs:range :Conserved_Name ;
                   
                   rdfs:domain :Rejected_Name ;
                   
                   rdfs:subPropertyOf :lacksPriorityInFavorOf .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#relatedPairsOfNames

:relatedPairsOfNames rdf:type owl:ObjectProperty .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#resultsInNomenclaturalStatus

:resultsInNomenclaturalStatus rdf:type owl:ObjectProperty ;
                              
                              rdfs:range :NomenclaturalActStatus ;
                              
                              rdfs:domain :NomenclaturalEventType ;
                              
                              rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.w3.org/2002/07/owl#topObjectProperty

owl:topObjectProperty rdf:type owl:ObjectProperty .



###  http://www.w3.org/2004/02/skos/core#hasBroader

skos:hasBroader rdf:type owl:ObjectProperty .



###  http://www.w3.org/2004/02/skos/core#related

skos:related rdf:type owl:ObjectProperty .



#################################################################
#
#    Classes
#
#################################################################


###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Absolute_Synonym

:Absolute_Synonym rdf:type owl:Class ; rdf:type skos:Concept ;
                  
                  rdfs:subClassOf :Nomen_Abortivum ;
                  
                  dc:description "A general term for synonyms based upon the same type.  "^^xsd:string ,
                                                                "Hawksworth: \"A homotypic synonym, a nomenclatural synonym (botany), an objective synonym (zoology). \""^^xsd:string ;
                  
                  rdfs:comment "Implied Junior/Later Absolute Synonym"^^xsd:string ;
                  
                  dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Acceptable_Under_Governing_Code

:Acceptable_Under_Governing_Code rdf:type owl:Class ; rdf:type skos:Concept ;
                                 
                                 rdfs:subClassOf :NomenclaturalActStatus ;
                                 
                                 owl:disjointWith :Not_Acceptable_Under_Governing_Code ;
                                 
                                 rdfs:comment "Statuses of nomenclatural acts that are acceptable statuses under the governing code."^^xsd:string ;
                                 
                                 dc:type "Category" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Accepted_Name

:Accepted_Name rdf:type owl:Class ; rdf:type skos:Concept ;
               
               rdfs:subClassOf :TaxonomicEventStatus ;
               
               dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Alternative_name

:Alternative_name rdf:type owl:Class ; rdf:type skos:Concept ;
                  
                  rdfs:subClassOf :NomenclaturalActStatus ;
                  
                  rdfs:comment "ICBN"^^xsd:string ;
                  
                  dc:description "One of a set of two or more different names proposed at the same time by the same author for the same taxon."^^xsd:string ;
                  
                  dc:title "nom. alt."^^xsd:string ;
                  
                  dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Ambiguuous_Name

:Ambiguuous_Name rdf:type owl:Class ; rdf:type skos:Concept ;
                 
                 rdfs:subClassOf :Not_Acceptable_Under_Governing_Code ;
                 
                 dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Approved_Name_ICNP

:Approved_Name_ICNP rdf:type owl:Class ; rdf:type skos:Concept ;
                    
                    rdfs:subClassOf :Acceptable_Under_Governing_Code ;
                    
                    rdfs:comment "ICNP Approved Name"^^xsd:string ;
                    
                    dc:title "nomen approbatum"^^xsd:string ;
                    
                    dc:type "Term" ;
                    
                    dc:title "nomen approbatum"@la .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Autonym_Avoidance

:Autonym_Avoidance rdf:type owl:Class ; rdf:type skos:Concept ;
                   
                   rdfs:subClassOf :Replacement_of_a_name ;
                   
                   dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Available_ICZN

:Available_ICZN rdf:type owl:Class ; rdf:type skos:Concept ;
                
                rdfs:subClassOf :Acceptable_Under_Governing_Code ;
                
                dc:type "Term" ;
                
                dc:description "Nomenclatural act statuses that are available names under the International Code of Zoological Nomenclature." .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Change_of_Type

:Change_of_Type rdf:type owl:Class ; rdf:type skos:Concept ;
                
                rdfs:subClassOf :NomenclaturalEventType ;
                
                dc:type "Category" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Change_of_combination

:Change_of_combination rdf:type owl:Class ; rdf:type skos:Concept ;
                       
                       rdfs:subClassOf :NomenclaturalEventType ;
                       
                       dc:type "Category" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Changed_Combination_ICZN

:Changed_Combination_ICZN rdf:type owl:Class ; rdf:type skos:Concept ;
                          
                          rdfs:subClassOf :Change_of_combination ,
                                          :TaxonomicEventType ;
                          
                          dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Conservation

:Conservation rdf:type owl:Class ; rdf:type skos:Concept ;
              
              rdfs:subClassOf :NomenclaturalEventType ;
              
              dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Conserved_Name

:Conserved_Name rdf:type owl:Class ; rdf:type skos:Concept ;
                
                rdfs:subClassOf :Acceptable_Under_Governing_Code ;
                
                rdfs:comment "A name that should not be acceptable under the governing code, but which has been conserved by an action of the relevant code governed body."^^xsd:string ;
                
                :example "Hemerobius elegans Guérin-Méneville, 1844"^^xsd:string ;
                
                dc:title "nomen conservandum"^^xsd:string ;
                
                dc:type "Term" ;
                
                dc:title "nomen conservandum"@la .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Corrected_Name

:Corrected_Name rdf:type owl:Class ; rdf:type skos:Concept ;
                
                rdfs:subClassOf :Acceptable_Under_Governing_Code ;
                
                dc:title "nomen correctum"^^xsd:string ;
                
                dc:type "Term" ;
                
                dc:title "nomen correctum"@la .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Creation_of_Justifed_Emendation

:Creation_of_Justifed_Emendation rdf:type owl:Class ; rdf:type skos:Concept ;
                                 
                                 rdfs:subClassOf :Replacement_of_a_name ;
                                 
                                 dc:title "ICZN"^^xsd:string ;
                                 
                                 dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Creation_of_Unjustified_Emendation

:Creation_of_Unjustified_Emendation rdf:type owl:Class ; rdf:type skos:Concept ;
                                    
                                    rdfs:subClassOf :Replacement_of_a_name ;
                                    
                                    rdfs:comment "ICZN:  Any change in spelling other than a Justified Emendation"^^xsd:string ;
                                    
                                    dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Doubtfull_Name

:Doubtfull_Name rdf:type owl:Class ; rdf:type skos:Concept ;
                
                rdfs:subClassOf :Dubious_Name ;
                
                rdfs:comment "ICNP.   A name of doubtfull or uncertain application."^^xsd:string ;
                
                dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Dubious_Name

:Dubious_Name rdf:type owl:Class ; rdf:type skos:Concept ;
              
              rdfs:subClassOf :NomenclaturalActStatus ;
              
              rdfs:comment """Hawksworth:  \"A name of unknown or doubtfull application.\"  

 Names for which the available circumscription information is too limited to allow the name to be related to a taxonomic concept."""^^xsd:string ;
              
              dc:title "nomen dubium"^^xsd:string ;
              
              dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Earlier_Homonym

:Earlier_Homonym rdf:type owl:Class ; rdf:type skos:Concept ;
                 
                 rdfs:subClassOf :Valid_ICBN ;
                 
                 rdfs:comment "ICBN"^^xsd:string ;
                 
                 dc:type "Term" ;
                 
                 rdfs:seeAlso :Later_Homonym .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Earlier_Nomenclatural_Synonym

:Earlier_Nomenclatural_Synonym rdf:type owl:Class ; rdf:type skos:Concept ;
                               
                               rdfs:subClassOf :Valid_ICBN ;
                               
                               dc:type "Term" ;
                               
                               rdfs:seeAlso :Later_Nomenclatural_Synonym .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Earlier_Taxonomic_Synonym

:Earlier_Taxonomic_Synonym rdf:type owl:Class ; rdf:type skos:Concept ;
                           
                           rdfs:subClassOf :Accepted_Name ;
                           
                           rdfs:comment """\"a name based on a different type from that of the accepted name of taxon; a subjective synonym in zool. and prok.; see also taxonomic synonym; syntaxonomic synonym.\"
Hawksworth, Terms Used in Bionomenclature."""^^xsd:string ;
                           
                           dc:type "Term" ;
                           
                           rdfs:seeAlso :Later_Taxonomic_Synonym .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Effectively_Published

:Effectively_Published rdf:type owl:Class ; rdf:type skos:Concept ;
                       
                       rdfs:subClassOf :Published ;
                       
                       :example "A work has met the criteria of the ICBN or ICNP to have been published."^^xsd:string ;
                       
                       dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#First_Revisor_Choosing_Spelling

:First_Revisor_Choosing_Spelling rdf:type owl:Class ; rdf:type skos:Concept ;
                                 
                                 rdfs:subClassOf :Replacement_of_a_name ;
                                 
                                 dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#First_Revisor_Chosing_Priority

:First_Revisor_Chosing_Priority rdf:type owl:Class ; rdf:type skos:Concept ;
                                
                                rdfs:subClassOf :NomenclaturalEventType ;
                                
                                dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Forgotten_Name

:Forgotten_Name rdf:type owl:Class ; rdf:type skos:Concept ;
                
                rdfs:subClassOf :Nomen_Oblitum ;
                
                rdfs:comment "ICZN Glossary: nomen oblitum: applied after 1 January 2000 to a name, unused since 1899, which as a result of an action taken under Article 23.9.2 does not take precedence over a younger synonym or homonym in prevailing usage; the younger name which takes precedence over the nomen oblitum may be called a nomen protectum (q.v.). "^^xsd:string ;
                
                dc:title "nomen oblitum"^^xsd:string ;
                
                dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Historical_Synonym

:Historical_Synonym rdf:type owl:Class ; rdf:type skos:Concept ;
                    
                    rdfs:subClassOf :Not_Acceptable_Under_Governing_Code ;
                    
                    rdfs:comment "ICNP.  Synonyms not included in the Approved List.  Generally placed in single quotes when included in a synonymy.  "^^xsd:string ;
                    
                    dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#ICBN

:ICBN rdf:type owl:Class ; rdf:type skos:Concept ;
      
      rdfs:label "International Code of Botanical Nomenclature"@en ;
      
      rdfs:subClassOf :NomenclaturalCode ;
      
      dc:title "International Code of Botanical Nomenclature"^^xsd:string ;
      
      dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#ICNP

:ICNP rdf:type owl:Class ; rdf:type skos:Concept ;
      
      rdfs:label "International Code for the Nomenclature of Proctists"@en ;
      
      rdfs:subClassOf :NomenclaturalCode ;
      
      dc:title "International Code of Nomenclature of Prokariotes.  Formerly International Code of Nomenclature of Bacteria. "^^xsd:string ;
      
      dc:type "Term" .



###  http://www.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#ICZN

:ICZN rdf:type owl:Class ; rdf:type skos:Concept ;
      
      rdfs:label "International Code of Zoological Nomenclature"@en ;
      
      rdfs:subClassOf :NomenclaturalCode ;
      
      dc:title "International Code of Zoological Nomenclature"^^xsd:string ;
      
